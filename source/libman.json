{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.1.0",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "vconsole@3.3.0",
      "destination": "wwwroot/lib/vconsole"
    },
    {
      "library": "jquery.qrcode@1.0",
      "destination": "wwwroot/lib/jquery.qrcode"
    }
  ]
}